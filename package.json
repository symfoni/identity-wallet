{
    "name": "identitywallet",
    "version": "0.0.2",
    "private": true,
    "scripts": {
        "android": "yarn env:dev && react-native run-android",
        "ios": "yarn ios:dev",
        "ios:dev": "yarn env:dev && react-native run-ios --scheme id.symfoni.app.dev --configuration id.symfoni.app.dev.Debug",
        "ios:stage": "yarn env:stage && react-native run-ios --scheme id.symfoni.app.stage --configuration id.symfoni.app.stage.Debug",
        "ios:prod": "yarn env:prod && react-native run-ios --scheme id.symfoni.app --configuration id.symfoni.app.Debug",
        "start": "yarn start:dev",
        "start:dev": "yarn env:dev && react-native start",
        "start:stage": "yarn env:stage && react-native start",
        "start:prod": "yarn env:prod && react-native start",
        "env": "./env.sh dev",
        "env:dev": "./env.sh dev",
        "env:stage": "./env.sh stage",
        "env:prod": "./env.sh prod",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && rm -rf node_modules/.cache/babel-loader",
        "postinstall": "rn-nodeify --hack --install assert,buffer,process,crypto,stream,vm && cd ios && pod install"
    },
